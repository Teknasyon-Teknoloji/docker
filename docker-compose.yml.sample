[PROJECT NAME]:
  image: [IMAGE NAME]
  ports:
    - "8080:80"
    - "3306:3306"
  environment:
    APPLICATION_ENV: development
    DB_NAME: [PROJECT DBNAME]
  command: sh /init.sh
  volumes:
    - ./config-files/pinfo.php:/pinfo.php
    - ./config-files/init.sh:/init.sh
    - ./config-files/supervisord.conf:/supervisord.conf
    - ./config-files/my.cnf:/etc/my.cnf
    - ./config-files/vhost.conf:/etc/nginx/conf.d/vhost.conf
    - ./config-files/www.conf:/etc/php-fpm.d/www.conf
    - ./config-files/php-development.ini:/etc/php.d/php-development.ini
    - ./../:/data/project
    - ./../log/nginx:/var/log/nginx
    - ./../log/php-fpm:/var/log/php-fpm
    - /var/lib/mysql
    - /root/.bash_history